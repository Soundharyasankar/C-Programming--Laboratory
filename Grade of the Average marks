#include<stdio.h>
int main()
{
    int tam,eng,mat,sci,soc;
    scanf("%d%d%d%d%d",&tam,&eng,&mat,&sci,&soc);
    int marks;
    float total=tam+eng+mat+sci+soc;
    float avg=total/5.0;
    if((tam<0||tam>100)||(eng<0||eng>100)||(mat<0||mat>100)||(sci<0||sci>100)
    ||(soc<0||soc>100))
    {
        printf("Invalid");
    }
    else if(tam<50||eng<50||mat<50||sci<50||soc<50)
    {
        printf("Fail");
    }
    else if(tam>50&&eng>50&&mat>50&&sci>50&&soc>50)
    {
        printf("Pass");
    }
    if(avg>=91&&avg<=100)
    {
        printf("O Grade");
    }
    else if(avg>=81&&avg<=90)
    {
        printf("A+ Grade");
    }
    else if (avg>=71&&avg<=80)
    {
        printf("A Grade");
    }
    else if(avg>=61&&avg<=70)
    {
        printf("B+ Grade");
    }
    else if (avg>=51&&avg<=60)
    {
        printf("B Grade");
    }
return 0;
}
