#include<stdio.h>
int main()
{
int n1,n2,n3;
scanf("%d%d%d",&n1,&n2,&n3);
if(n1<n2&&n1<n3)
{
    printf("%d is smaller",n1);
}
else if(n2<n1&&n2<n3)
{
    printf("%d is smaller",n2);
}
else if(n3<n1 && n3<n1)
{
    printf("%d is smaller",n3);
}
else if (n1==n2==n3)
printf("All are equal");
return 0;
}
